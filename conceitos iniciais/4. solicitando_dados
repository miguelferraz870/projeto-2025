import os



#limpa o terminal.
os.system("clear")

# solicitar dados para o usurario.

nome = input("Digite seu nome: ")
idade = int(input("Digite sua idade: "))
altura = float(input("Digite sua altura: "))



# Exibindo dados.
print(f"nome: {nome}")
print(f"Idade: {idade}")
print(f"Altura: {altura}")






